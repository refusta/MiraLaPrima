desc "This task is called to refesh the prima values"

task :GetPrima do
    time = Time.new
    puts "Day of week"
    puts time.wday
    if time.wday != 6 || time.wday != 0
        require 'open-uri'
        open('http://miralaprima.herokuapp.com/GetPrima')
        puts "Updating prima..."
    end
end